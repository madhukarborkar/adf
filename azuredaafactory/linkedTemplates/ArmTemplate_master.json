{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredaafactory"
		},
		"link_azure_data_lakes_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'link_azure_data_lakes'"
		},
		"link_azure_data_lakes_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mbadlsstroageaccount.dfs.core.windows.net/"
		},
		"rest_country_all_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/all"
		},
		"county_wise_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://restcountries.com/v3.1/name"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredaafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"link_azure_data_lakes_accountKey": {
						"value": "[parameters('link_azure_data_lakes_accountKey')]"
					},
					"link_azure_data_lakes_properties_typeProperties_url": {
						"value": "[parameters('link_azure_data_lakes_properties_typeProperties_url')]"
					},
					"rest_country_all_properties_typeProperties_url": {
						"value": "[parameters('rest_country_all_properties_typeProperties_url')]"
					},
					"county_wise_ls_properties_typeProperties_url": {
						"value": "[parameters('county_wise_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}